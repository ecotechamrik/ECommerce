@model BAL.ViewModels.Product.DoorTypeViewModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_ProductLayout.cshtml";
}

<h1>Door Type Details</h1>

<div>
    <a asp-action="Index"><customicon type="back" title="Back to Door Types"></customicon> </a> |
    <a href="@BAL.Common.WEBSITEPATH/product/doortype/edit/@Model.DoorTypeID"><customicon type="edit" title="Edit Door Type"></customicon></a> |
    <a href="@BAL.Common.WEBSITEPATH/product/doortype/delete/@Model.DoorTypeID" onclick="return confirm('Do you want to delete this record?')"><customicon type="delete" title="Delete Door Type"></customicon></a>
</div>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-3">
            @Html.DisplayNameFor(model => model.DoorTypeName)
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.DoorTypeName)
        </dd>
    </dl>
</div>