@model BAL.ViewModels.Product.DoorTypeViewModel

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_ProductLayout.cshtml";
}

<h1>Create</h1>
<div>
    <a asp-action="Index"><customicon type="back" title="Back to Door Types"></customicon></a>
</div>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <input type="hidden" asp-for="DoorTypeID" />
                <label asp-for="DoorTypeName" class="control-label"></label>
                <input asp-for="DoorTypeName" class="form-control" />
                <span asp-validation-for="DoorTypeName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Submit" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>