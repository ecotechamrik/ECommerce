@model BAL.ViewModels.Product.ProductWidthViewModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_ProductLayout.cshtml";
}

    <h1>Product Width Details</h1>

<div>
    <a asp-action="Index"><customicon type="back" title="Back to Product Widths"></customicon> </a> |
    <a href="@BAL.Common.WEBSITEPATH/product/productWidth/edit/@Model.ProductWidthID"><customicon type="edit" title="Edit Product Width"></customicon></a> |
    <a href="@BAL.Common.WEBSITEPATH/product/productWidth/delete/@Model.ProductWidthID" onclick="return confirm('Do you want to delete this record?')"><customicon type="delete" title="Delete Product Width"></customicon></a>
</div>

<div>    
    <hr />
    <dl class="row">
        <dt class="col-sm-3">
            @Html.DisplayNameFor(model => model.ProductWidthName)
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.ProductWidthName)
        </dd>
    </dl>
</div>