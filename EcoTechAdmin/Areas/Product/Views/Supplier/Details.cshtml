@model BAL.ViewModels.Product.SupplierViewModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_ProductLayout.cshtml";
}

<h1>Door Type Details</h1>

<div>
    <a asp-action="Index"><customicon type="back" title="Back to Suppliers"></customicon> </a> |
    <a href="@BAL.Common.WEBSITEPATH/product/supplier/edit/@Model.SupplierID"><customicon type="edit" title="Edit Supplier"></customicon></a> |
    <a href="@BAL.Common.WEBSITEPATH/product/supplier/delete/@Model.SupplierID" onclick="return confirm('Do you want to delete this record?')"><customicon type="delete" title="Delete Supplier"></customicon></a>
</div>

