@model BAL.ViewModels.Product.ProductThicknessViewModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_ProductLayout.cshtml";
}

    <h1>Product Thickness Details</h1>

<div>
    <a asp-action="Index"><customicon type="back" title="Back to Product Thicknesss"></customicon> </a> |
    <a href="@BAL.Common.WEBSITEPATH/product/productThickness/edit/@Model.ProductThicknessID"><customicon type="edit" title="Edit Product Thickness"></customicon></a> |
    <a href="@BAL.Common.WEBSITEPATH/product/productThickness/delete/@Model.ProductThicknessID" onclick="return confirm('Do you want to delete this record?')"><customicon type="delete" title="Delete Product Thickness"></customicon></a>
</div>

<div>    
    <hr />
    <dl class="row">
        <dt class="col-sm-3">
            @Html.DisplayNameFor(model => model.ProductThicknessName)
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.ProductThicknessName)
        </dd>
    </dl>
</div>