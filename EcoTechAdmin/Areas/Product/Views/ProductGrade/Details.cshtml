@model BAL.ViewModels.Product.ProductGradeViewModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_ProductLayout.cshtml";
}

<h1>Product Grade Details</h1>

<div>
    <a asp-action="Index"><customicon type="back" title="Back to Product Grades"></customicon> </a> |
    <a href="@BAL.Common.WEBSITEPATH/product/productgrade/edit/@Model.ProductGradeID"><customicon type="edit" title="Edit Product Grade"></customicon></a> |
    <a href="@BAL.Common.WEBSITEPATH/product/productgrade/delete/@Model.ProductGradeID" onclick="return confirm('Do you want to delete this record?')"><customicon type="delete" title="Delete Product Grade"></customicon></a>
</div>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-3">
            @Html.DisplayNameFor(model => model.ProductGradeName)
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.ProductGradeName)
        </dd>
    </dl>
</div>