@model BAL.ViewModels.Product.ProductDesignViewModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_ProductLayout.cshtml";
}

<h1>Product Design Details</h1>

<div>
    <a asp-action="Index"><customicon type="back" title="Back to Product Designs"></customicon> </a> |
    <a href="@BAL.Common.WEBSITEPATH/product/productdesign/edit/@Model.ProductDesignID"><customicon type="edit" title="Edit Product Design"></customicon></a> |
    <a href="@BAL.Common.WEBSITEPATH/product/productdesign/delete/@Model.ProductDesignID" onclick="return confirm('Do you want to delete this record?')"><customicon type="delete" title="Delete Product Design"></customicon></a>
</div>

<div>    
    <hr />
    <dl class="row">
        <dt class="col-sm-3">
            @Html.DisplayNameFor(model => model.ProductDesignName)
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.ProductDesignName)
        </dd>
    </dl>
</div>