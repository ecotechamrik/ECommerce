@model BAL.ViewModels.Product.ProductHeightViewModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_ProductLayout.cshtml";
}

    <h1>Product Height Details</h1>

<div>
    <a asp-action="Index"><customicon type="back" title="Back to Product Heights"></customicon> </a> |
    <a href="@BAL.Common.WEBSITEPATH/product/productheight/edit/@Model.ProductHeightID"><customicon type="edit" title="Edit Product Height"></customicon></a> |
    <a href="@BAL.Common.WEBSITEPATH/product/productheight/delete/@Model.ProductHeightID" onclick="return confirm('Do you want to delete this record?')"><customicon type="delete" title="Delete Product Height"></customicon></a>
</div>

<div>    
    <hr />
    <dl class="row">
        <dt class="col-sm-3">
            @Html.DisplayNameFor(model => model.ProductHeightName)
        </dt>
        <dd class="col-sm-9">
            @Html.DisplayFor(model => model.ProductHeightName)
        </dd>
    </dl>
</div>